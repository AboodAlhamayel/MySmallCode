#include<iostream>
#include<iomanip>
#include "MyLib.h"
#include "SumLib.h"
#include<cstdio>
#include<vector>
using namespace std;

void Swaping(int* n1, int* n2)
{
    int temp;
    temp = *n1;
    *n1 = *n2;
    *n2 = temp;
}

int main()
{
    int a = 1, b = 2;

    cout << "\nBefore swapping : " << endl;
    cout << "a = " << a << endl;
    cout << "b = " << b << endl;

    Swaping(&a, &b);

    cout << "\nAfter swapping : " << endl;
    cout << "a = " << a << endl;
    cout << "b = " << b << endl;

    return 0;
}
