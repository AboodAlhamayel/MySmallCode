#include<iostream>
#include<iomanip>
#include "MyLib.h"
#include "SumLib.h"
#include<cstdio>
#include<vector>
using namespace std;

int main()
{
    int a = 10;
    int& x = a;

    cout << &a << endl;
    cout << &x << endl;

    cout << a << endl;
    cout << x << endl;

    a = 20;

    cout << a << endl;
    cout << x << endl;

    return 0;
}
